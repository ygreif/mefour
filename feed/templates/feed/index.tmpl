	  <!-- Navigation -->
	    <nav class="navbar navbar-fixed-top navbar-inverse" role="navigation">
			<div class="container-fluid">

				<!-- Brand and toggle get grouped for better mobile display -->
				<div class="navbar-header">
					<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand" href="#">{{ site }}</a>
				</div>
				<!-- /.navbar-header -->

				<!-- Collect the nav links, forms, and other content for toggling -->
				<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
					<ul class="nav navbar-nav">
						<li><a href="#">About</a></li>
						<li><a href="mailto:contact@advicebee.com">Contact</a></li>
					</ul>
				</div>
				<!-- /.navbar-collapse -->
			</div>
			<!-- /.container-fluid -->
		</nav>
		<!-- /.navbar -->

		<!-- Page Content -->
		<div class="container-fluid">
			<div class="row">
				<div class="col-sm-12">
					<div class="page-header">
						<h1>{{ site }} - {{ tagline }}</h1>
					</div>
				</div>
			</div>
			<!-- /.row -->


                        <div class="row margin-b-2">
			  <div class="col-sm-12 col-lg-8">
                            <a href="{{ lead.link_url }}" id="{{ lead.mixpanelid }}" target="_blank">
			      <img class="img-responsive thumbnail" src="{{ lead.ImgUrl }}" alt="">
                            </a>
                          </div>
                          <div class="col-sm-12 col-lg-4">
			    <div class="caption">
			      <h1><a href="{{ lead.link_url }}" id="{{ lead.mixpanelid }}" target="_blank" class="lead-title">{{ lead.Title }}</a></h1>
                              <p class="source">{{ lead.source }}</p>
			      <p class="teaser">{{ lead.Teaser }}</p>
			    </div>
			  </div>
			</div>
                        <script type="text/javascript">
                            mixpanel.track_links("#{{ lead.mixpanelid }}", "{{ lead.Title|striptags }}");
                        </script>


                        {% for row in rows %}
			<div class="row margin-b-2">
                          {% for entry in row %}
			  <div class="col-lg-4">
                            <div class="row">
                              <div class="col-sm-4 col-lg-12">
                                <a class="img-link" href="{{ entry.link_url }}" id="{{ entry.mixpanelid }}" target="_blank">
			          <img class="img-responsive thumbnail" src="{{ entry.ImgUrl }}">
                                </a>
                              </div>
                              <div class="col-sm-8 col-lg-12">
			        <div class="caption">
				  <h2><a href="{{ entry.link_url }}" id="{{ entry.mixpanelid }}" target="_blank" class="story-title">{{ entry.Title|striptags }}</a></h2>
                                  <p class="source"> {{ entry.source }} <p class="source">{{ entry.pub_date | date }} </p>
				  <p class="teaser">{{ entry.Teaser }}</p>
                                </div>
			      </div>
                            </div>
			  </div>
                          <script type="text/javascript">
                            mixpanel.track_links("#{{ entry.mixpanelid }}", "{{ entry.Title|striptags }}");
                          </script>

                          
                          {% endfor %}
			</div>
                        {% endfor %}
			<!-- /.row -->

			<hr>
			<footer class="margin-tb-3">
				<div class="row">
					<div class="col-lg-12">
						<p>Copyright NA</p>
					</div>
				</div>
			</footer>
		</div>
		<!-- /.container-fluid -->

